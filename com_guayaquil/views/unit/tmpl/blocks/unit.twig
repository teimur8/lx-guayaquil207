{% use 'blocks/unitHeader.twig' %}
{% use 'blocks/unitRemark.twig' %}
{% use 'blocks/image.twig' %}
{% use 'blocks/details.twig' %}
{% use 'blocks/legend.twig' %}

{% block unit %}
    <div class="unit_page">
        {{ block('unit_header') }}
        <div class="unit" id="g_container">
            {{ block('unit_remark') }}
            <div class="tim-grid tim-grid-50">
                <div class="tim-grid_sticky">
                    {{ block('image') }}
                    {{ block('legend') }}
                </div>
                {{ block('details') }}
            </div>
        </div>
    </div>
{% endblock %}